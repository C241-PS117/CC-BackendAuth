generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  idUser        Int      @id @default(autoincrement())
  namalengkap   String
  email         String   @unique
  jenis_kelamin String
  password      String
  gambarprofil  String?
}